    "system-bell-webpack-plugin": "1.0.0",
    "url-loader": "1.0.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.0"
  },
  "jest": {
    "testRegex": "/*.test.js$",
    "collectCoverage": true
  }
