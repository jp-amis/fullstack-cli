{
  "name": "{{ name }}",
  "version": "{{ version }}",
  "description": "{{ description }}",
  "main": "src/app/index.js",
  "author": "{{{ author }}} <{{{ email }}}>",
  "license": "{{ license }}",
  "private": {{ private }},
