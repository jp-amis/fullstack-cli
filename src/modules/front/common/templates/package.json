    "react-redux": "5.1.0",
    {{/ redux }}
    {{# routes}}
    "react-router-dom": "4.3.1",
    {{/ routes}}
    {{# material-ui }}
    "material-ui": "0.20.2",
    {{/ material-ui }}
    {{# websockets }}
    "socket.io-client": "2.1.1",
    {{/ websockets }}
    "classnames": "2.2.6",
    "image-trace-loader": "0.1.0",
    "moment": "2.22.2",
    "numeral": "2.0.6",
    "superagent": "3.8.3"
