{
  "name": "{{ name }}",
  "version": "{{ version }}",
  "description": "{{ description }}",
  "main": "src/app/index.js",
  "author": "{{{ author }}} <{{{ email }}}>",
  "license": "{{ license }}",
  "private": {{ private }},
  "scripts": {
    "start": "webpack-dev-server --env.dev",
    "test": "npm run eslint && npm run stylelint && npm run flow{{#redux}} && jest{{/redux}}",
    "build": "npm test && webpack --env.prod",
    "docs": "esdoc -c esdoc.json",
    "eslint": "eslint src",
    "stylelint": "stylelint src",
    "flow": "flow"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "websockets",
    "material-ui"
  ],
  "dependencies": {
    "react": "16.6.0",
    "react-dom": "16.6.0",
